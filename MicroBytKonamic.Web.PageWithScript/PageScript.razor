@inject NavigationManager navigationManager

@{
    var src = navigationManager.GetUriWithQueryParameters(Src, Params);
}

<page-script src="@Src"></page-script>

@code {
    [Parameter, EditorRequired] public string Src { get; set; } = default!;
    [Parameter(CaptureUnmatchedValues = true)] public Dictionary<string, object>? Params { get; set; }
}
